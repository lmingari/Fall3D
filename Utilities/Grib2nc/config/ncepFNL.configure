!
!  Grib2nc configure file for GFS FNL at 1 degree resolution
!
lonmin -180    ! Only used if the model does not give RLON
lonmax  180    ! Only used if the model does not give RLON
latmin  -90    ! Only used if the model does not give RLAT
latmax   90    ! Only used if the model does not give RLAT
nx  360
ny  181
np  26      ! Number pressure levels
nt 1        ! Number of time steps
dt  6       ! time increment in h
n2d  8      ! Number of 2d variables  grb --> nc
n3d  6      ! Number of 3d variables  grb --> nc
cen_lon  0.
cen_lat  0.
missing_value 9.999e+20        ! missing value
PRESSURE LEVELS
1000
975
950
925
900
850
800
750
700
650
600
550
500
450
400
350
300
250
200
150
100
70
50
30
20
10
VARIABLES 2D
var2d(1)    'UGRD:10'
att2d_d(1)  'u-component (zonal) of wind at 10m'
att2d_u(1)  'm/s'
var2d(2)    'VGRD:10'
att2d_d(2)  'v-component (meridional) of wind at 10m'
att2d_u(2)  'm/s'
var2d(3)    'TMP:2'
att2d_d(3)  'temperature at 2m'
att2d_u(3)  'K'
var2d(4)    'LAND'
att2d_d(4)  'Land mask'
att2d_u(4)  '-'
var2d(5)    'PRES:surface'
att2d_d(5)  'pressure at surface'
att2d_u(5)  'Pa'
var2d(6)    'HGT:surface'
att2d_d(6)  'height'
att2d_u(6)  'm'
var2d(7)    'TMP:surface'
att2d_d(7)  'temperatrue at surface'
att2d_u(7)  'K'
var2d(8)    'HPBL'
att2d_d(8)  'Planetary Boundary Layer Height'
att2d_u(8)  'm'
VARIABLES 3D         
var3d(1)    'TMP'
att3d_d(1)  'temperature'
